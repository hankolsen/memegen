{
  "name": "@hank-iv/memegen",
  "version": "2.0.1",
  "author": "Hank",
  "license": "MIT",
  "description": "A meme generator in NodeJs",
  "keywords": [
    "meme",
    "memegenerator",
    "image",
    "fun"
  ],
  "bugs": {
    "url": "https://github.com/hankolsen/memegen/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hankolsen/memegen.git"
  },
  "homepage": "https://github.com/hankolsen/memegen#readme",
  "source": "src/index.js",
  "main": "lib/index.js",
  "bin": {
    "meme": "./bin/meme"
  },
  "scripts": {
    "eslint": "eslint .",
    "build": "parcel build && npm run cp:assets",
    "cp:assets": "cp -R src/assets lib/",
    "dev": "parcel watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  },
  "directories": {
    "lib": "lib",
    "bin": "bin"
  },
  "targets": {
    "default": {
      "context": "node",
      "engines": {
        "node": "18"
      }
    }
  },
  "engines": {
    "node": "18"
  },
  "dependencies": {
    "canvas": "^2.11.2",
    "img-clipboard": "^1.1.0",
    "meow": "^9.0.0"
  },
  "devDependencies": {
    "eslint": "^8.44.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "parcel": "^2.9.3"
  }
}
